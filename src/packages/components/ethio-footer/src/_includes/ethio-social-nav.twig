{% set social_nav = footer.secondary_section.social_nav %}

<div class="ethio-footer__social-links grid-row grid-gap-1">
  {% for item in social_nav %}
    <div class="grid-col-auto">
      {% set icon = (item == "RSS" ? "rss_feed" : item) %}
      <a class="ethio-social-link" href="javascript:void(0);">
        <img class="ethio-social-link__icon" src="./img/ethio-icons/{{ icon | lower }}.svg" alt="{{ item }}" />
      </a>
    </div>
  {% endfor %}
</div>
