<main class="ethio-section">
  <form class="grid-container">
    <fieldset class="ethio-fieldset">
      <legend class="ethio-sr-only">Historical figures 1</legend>
      <div class="grid-row grid-gap">
        <div class="ethio-checkbox tablet:grid-col-3">
          <input class="ethio-checkbox__input" id="washington" type="checkbox" name="historical-figures-1" value="washington">
          <label class="ethio-checkbox__label" for="washington">Booker T. Washington</label>
        </div>
        <div class="ethio-checkbox tablet:grid-col-3">
          <input class="ethio-checkbox__input" id="douglass" type="checkbox" name="historical-figures-1" value="douglass">
          <label class="ethio-checkbox__label" for="douglass">Frederick Douglass</label>
        </div>
        <div class="ethio-checkbox tablet:grid-col-3">
          <input class="ethio-checkbox__input" id="truth" type="checkbox" name="historical-figures-1" value="truth" checked>
          <label class="ethio-checkbox__label" for="truth">Sojourner Truth</label>
        </div>
        <div class="ethio-checkbox tablet:grid-col-3">
          <input class="ethio-checkbox__input" id="tubman" type="checkbox" name="historical-figures-1" value="tubman">
          <label class="ethio-checkbox__label" for="tubman">Harriet Tubman</label>
        </div>
      </div>
    </fieldset>
  </form>
</main>
